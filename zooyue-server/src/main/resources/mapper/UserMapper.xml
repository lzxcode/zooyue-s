<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="zooyue.cn.mapper.UserMapper">
    <select id="getUserByUser" parameterType="zooyue.cn.entity.User" resultType="zooyue.cn.entity.User">
        select * from zooyue.user
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
        </where>
    </select>
</mapper>
